}


_gpVelocityNode _gpVelocityTracker_gpVelocityNode_createInstance(gpMotionEvent* motionEvent) {
	_gpVelocityNode* node = gpAlloc_alloc(sizeof(_gpVelocityNode));
	node->event = motionEvent;
	node->next = null;
	node->previous = null;
	return node;
}

#endif
