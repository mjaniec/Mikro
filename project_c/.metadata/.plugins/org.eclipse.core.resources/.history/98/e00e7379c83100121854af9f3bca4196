#ifndef GPINSTANCE_H_
#define GPINSTANCE_H_

#include "../Base/gp.h"

struct gpInstance {
	// the feature vector
	gpFloat[] gpInstance_vector;

	// the label can be null
	gpString gpInstance_label;

	// the id of the instance
	gpWord gpInstance_id;

};

gpInstance* gpInstance_New(gpWord id, gpFloat[] sample, gpString);


#endif
