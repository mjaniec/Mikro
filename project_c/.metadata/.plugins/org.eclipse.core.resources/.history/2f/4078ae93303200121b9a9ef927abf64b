C=gcc
CFLAGS=${DEBUGINFO} -I"${INCLUDE}/Base" -I"${INCLUDE}/Math" -I"${INCLUDE}/Alloc" -D${MODE} -std=c99  -Wno-main -Wall

all: clean libGPAlloc.a 
clean:;	rm -f *.o *.exe *.a

libGPAlloc.a: gp_Alloc.o
	ar -r ${OUTPUT}\$@ $^
	
%.o:%.c
	gcc -c ${CFLAGS} -o $@ $<
	
GPAlloc : 
	gcc ${CFLAGS} -L"${OUTPUT}" -lGPAlloc -lGPBase -lGPMath -o test