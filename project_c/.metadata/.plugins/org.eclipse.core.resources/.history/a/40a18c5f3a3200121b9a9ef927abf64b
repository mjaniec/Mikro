CC=gcc
HEADERS=-I"${INCLUDE}/Base" -I"${INCLUDE}/Alloc" -I"${INCLUDE}/Math" -I"${INCLUDE}/Instance" -I"${INCLUDE}/InstanceLearner" -I"${INCLUDE}/InputGesture" -I"${INCLUDE}/OutputGesture" -I"${INCLUDE}/GestureStore" 
CFLAGS=${DEBUGINFO} ${HEADERS} -D${MODE} -std=c99  -Wno-main -Wall

all: gp_Main.s

gp_Main.o: Source/gp_Main.c
	${CC} ${CFLAGS} -o "${OUTPUT}\$@" $^
	
	
gp_Main.s: Source/gp_Main.c
	${CC} -E ${CFLAGS} -o "${OUTPUT}\$@" $^