CC=gcc
HEADERS=-I"${INCLUDE}/Base" -I"${INCLUDE}/Alloc" -I"${INCLUDE}/Math" -I"${INCLUDE}/Instance" -I"${INCLUDE}/InstanceLearner" -I"${INCLUDE}/Gestures" -I"${INCLUDE}/Gesture" -I"${INCLUDE}/GestureStore" 
CFLAGS=${DEBUGINFO} ${HEADERS} -D${MODE} -std=c99  -Wno-main -Wall

all: Source/gp_Main.s

Source/gp_Main.o: Source/gp_Main.c
	${CC} ${CFLAGS} -o "${OUTPUT}\$@" $^
	
	
Source/gp_Main.s: Source/gp_Main.c
	${CC} ${CFLAGS} -o "${OUTPUT}\$@" $^