#ifndef GPINSTANCE_H_
#define GPINSTANCE_H_
#include "../Base/gp_types.h"

typedef struct gpInstance {
	// the feature vector
	gpFloat* vector;

	// the label can be null
	gpString label;

	// the id of the instance
	gpWord id;
} gpInstance;

gpInstance* gpInstance_New(gpWord id, gpFloat[], gpString);

#endif
